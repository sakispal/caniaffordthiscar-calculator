{
  "name": "caniaffordthiscar",
  "version": "1.0.0",
  "description": "Car affordability calculator",
  "main": "app.js",
  "scripts": {
    "test": "export TEST=true; export NODE_ENV=production; mocha ./*.test.js",
    "test-dev" : "export TEST=true; export NODE_ENV=development; mocha ./*.test.js",
    "test-localdb" : "export TEST=true; export NODE_ENV=localdb; mocha ./*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "start": "export NODE_ENV=production; node app.js",
    "dev": "export NODE_ENV=development; node app.js",
    "dev-localdb": "export NODE_ENV=localdb; node app.js",
    "dev-watch": "nodemon --exec 'npm run dev'"
  },
  "author": "Sakis",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-minify": "^1.0.0",
    "express-minify-html": "^0.12.0",
    "express-sanitizer": "^1.0.4",
    "file-system": "^2.2.2",
    "helmet": "^3.13.0",
    "mongoose": "^5.7.5",
    "nodemailer": "^4.6.7",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "supertest": "^3.3.0",
    "typed.js": "^2.0.6",
    "x-ray": "^2.3.4"
  },
  "devDependencies": {
    "expect": "^23.6.0",
    "mocha": "^5.2.0"
  }
}
